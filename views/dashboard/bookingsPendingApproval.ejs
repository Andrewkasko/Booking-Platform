<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    <title>Blank</title>
    <style>#loader {
            transition: all .3s ease-in-out;
            opacity: 1;
            visibility: visible;
            position: fixed;
            height: 100vh;
            width: 100%;
            background: #fff;
            z-index: 90000
        }

        #loader.fadeOut {
            opacity: 0;
            visibility: hidden
        }

        .spinner {
            width: 40px;
            height: 40px;
            position: absolute;
            top: calc(50% - 20px);
            left: calc(50% - 20px);
            background-color: #333;
            border-radius: 100%;
            -webkit-animation: sk-scaleout 1s infinite ease-in-out;
            animation: sk-scaleout 1s infinite ease-in-out
        }

        @-webkit-keyframes sk-scaleout {
            0% {
                -webkit-transform: scale(0)
            }
            100% {
                -webkit-transform: scale(1);
                opacity: 0
            }
        }

        @keyframes sk-scaleout {
            0% {
                -webkit-transform: scale(0);
                transform: scale(0)
            }
            100% {
                -webkit-transform: scale(1);
                transform: scale(1);
                opacity: 0
            }
        }

    </style>
    <link href="public/style.css" rel="stylesheet">
</head>
<body class="app">
<div id="loader">
    <div class="spinner"></div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="functionality.js"></script>
<script>window.addEventListener('load', function load() {
        const loader = document.getElementById('loader');
        setTimeout(function () {
            loader.classList.add('fadeOut');
        }, 300);
    });</script>
<div>
    <div class="sidebar">
        <div class="sidebar-inner">
            <div class="sidebar-logo">
                <div class="peers ai-c fxw-nw">
                    <div class="peer peer-greed"><a class="sidebar-link td-n" href="/dashboard">
                            <div class="peers ai-c fxw-nw">
                                <div class="peer">
                                    <div class="logo"><img src="assets/static/images/medlogo.png" alt="" height="60"
                                                           width="60"></div>
                                </div>
                                <div class="peer peer-greed"><h5 class="lh-1 mB-0 logo-text">E-healthcare</h5></div>
                            </div>
                        </a></div>
                    <div class="peer">
                        <div class="mobile-toggle sidebar-toggle"><a href="" class="td-n"><i
                                        class="ti-arrow-circle-left"></i></a></div>
                    </div>
                </div>
            </div>
            <ul class="sidebar-menu scrollable pos-r">
                <li class="nav-item mT-30 actived"><a class="sidebar-link" href="/dashboard"><span
                                class="icon-holder"><i class="c-blue-500 ti-home"></i> </span><span
                                class="title">Dashboard</span></a></li>
                <li class="nav-item"><a class="sidebar-link" href="/approveABooking"><span class="icon-holder"><i
                                    class="c-deep-orange-500 ti-calendar"></i> </span><span class="title">Booking</span></a>
                </li>
                <li class="nav-item"><a class="sidebar-link" href="/blank"><span class="icon-holder"><i
                                    class="c-deep-purple-500 ti-comment-alt"></i> </span><span class="title">Chat</span></a>
                </li>
                <li class="nav-item dropdown"><a class="dropdown-toggle" href="javascript:void(0);"><span
                                class="icon-holder"><i class="c-purple-500 ti-map"></i> </span><span
                                class="title">Maps</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="page-container">
        <div class="header navbar">
            <div class="header-container">
                <ul class="nav-left">
                    <li><a id="sidebar-toggle" class="sidebar-toggle" href="javascript:void(0);"><i class="ti-menu"></i></a>
                    </li>
                </ul>
                <ul class="nav-right">
                    <li class="dropdown-toggle no-after peers fxw-nw ai-c lh-1">
                        <img style="margin-top: 15px; margin-right: 10px" class="w-2r bdrs-50p"src="data:image/png;base64," alt="">

                    </li>
                    <li class="dropdown-toggle no-after peers fxw-nw ai-c lh-1">
                        <span style="margin-right: 30px; margin-top: 20px"><a href="/UpdateUser"><%= name %></a></span>
                    </li>
                </ul>
            </div>
        </div>
        <main class="main-content bgc-grey-100">
            <div id="mainContent">
                <div class="full-container">

                    <div class="masonry-item col-md-12">
                        <div class="bd bgc-white p-20">
                            <div class="layers">
                                <div class="layer w-100 mB-10">
                                    <div class="form-row">
                                        <div class="col-md-6">
                                            <h2 class="lh-1">Manage Bookings</h2>
                                        </div>
                                    </div>
                                    <div class="topnav w-100" style="margin-top:20px; margin-bottom: 20px">
                                        <a href="/createABooking">Create A Booking</a>
                                        <a href="/UpcomingBookingsPatient">Upcoming Bookings</a>
                                        <a href="/bookingPatient">All Bookings</a>
                                        <a class="active" href="/bookingsPendingApproval">Bookings Pending Approval</a>
                                    </div>
                                    <div class="form-row">
                                        <div class="col-md-6">
                                            <h2 class="lh-1">Pending Bookings</h2>
                                        </div>
                                    </div>
                                </div>
                                <div class="w-100">
                                    <%- include('../partials/messages') %>
                                </div>

                                <div class="layer w-100" style="overflow-y: scroll; height:80vh;">
                                    <% for(var booking of bookings){ %>
                                        <div class="card" style="width: 100%;">
                                            <div class="card-body">

                                                <div class="form-row">
                                                    <div class="col-md-6">
                                                        <h4 class="card-title"
                                                            style="color:black;"><%= booking.name + " - " + booking.topic %></h4>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <% if(booking.bookingType == "Emergency") { %>

                                                            <div style=" width: 100%; border: 5px solid darkred; background: repeating-linear-gradient(45deg, red, red 10px, darkred 10px, darkred 20px);
                                                            border-radius: 25px; padding: 10px;">
                                                                <h5 style="text-align: center!important; color:white; margin: auto;">
                                                                    Extreme Case</h5>
                                                            </div>
                                                        <% }else{ %>
                                                            <div style=" width: 100%; border: 5px solid orangered; background: orange;
                                                             border-radius: 25px; padding: 10px;">
                                                                <h5 style="text-align: center!important; color:white; margin: auto;">
                                                                    Pending Doctors Approval</h5>
                                                            </div>
                                                        <% } %>
                                                    </div>
                                                </div>
                                                <div class="form-row">

                                                    <div class="col-md-12">
                                                        <h6 class="card-subtitle mb-2 text-muted">
                                                            Booking Date: <%= booking.date %>
                                                        </h6>
                                                    </div>
                                                </div>
                                                <div class="form-row">

                                                    <div class="col-md-12">
                                                        <h6 class="card-subtitle mb-2 text-muted">
                                                            Booking Time: <%= booking.time %>
                                                        </h6>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="col-md-12">
                                                        <h6 class="card-subtitle mb-2 text-muted">
                                                            <%= "Doctor: " + booking.doctorName %>
                                                        </h6>
                                                    </div>
                                                </div>
                                                <div class="form-row">
                                                    <div class="col-md-12">
                                                        <h6 class="card-subtitle mb-2 text-muted">
                                                            <%= "Booking Topic: " + booking.topic %>
                                                        </h6>
                                                    </div>
                                                </div>

                                                <p class="card-text">
                                                    Booking Description: <%= booking.description %>
                                                </p>
                                            </div>
                                        </div>
                                        <br>
                                    <% } %>
                                </div>

                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </main>
    </div>
</div>
<script type="text/javascript" src="public/vendor.js"></script>
<script type="text/javascript" src="public/bundle.js"></script>
</body>
</html>